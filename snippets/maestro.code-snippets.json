    "prefix": ["at", "asserttrue"],
    "body": [
      "- assertTrue: \"$1\""
    ],
    "description": "Assert expression is true"
  },
  "assertFalse": {
    "prefix": ["af", "assertfalse"],
    "body": [
      "- assertFalse: \"$1\""
    ],
    "description": "Assert expression is false"
  },
  "assertThat": {
    "prefix": ["ath"],
    "body": [
