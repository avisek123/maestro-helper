{
  "tapOn": {
    "prefix": ["ta", "tap"],
    "body": [
      "- tapOn:",
      "    id: \"$1\""
    ],
    "description": "Tap on an element by testID"
  },
  "tapOnText": {
    "prefix": ["tat", "taptext"],
    "body": [
      "- tapOn:",
      "    text: \"$1\""
    ],
    "description": "Tap on an element by text"
  },
  "assertVisible": {
    "prefix": ["av"],
    "body": [
      "- assertVisible:",
      "    id: \"$1\""
    ],
    "description": "Assert element is visible"
  },
  "assertVisibleText": {
    "prefix": ["avt"],
    "body": [
      "- assertVisible:",
      "    text: \"$1\""
    ],
    "description": "Assert element is visible by text"
  },
  "assertNotVisible": {
    "prefix": ["anv"],
    "body": [
      "- assertNotVisible:",
      "    id: \"$1\""
    ],
    "description": "Assert element is not visible"
  },
  "launchApp": {
    "prefix": ["la", "launch"],
    "body": [
      "- launchApp:",
      "    appId: \"$1\""
    ],
    "description": "Launch app with appId"
