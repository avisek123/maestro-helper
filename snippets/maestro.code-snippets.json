  },
  "extendState": {
    "prefix": ["exs", "extendstate"],
    "body": [
      "- extendState:",
      "    $1: \"$2\""
    ],
    "description": "Extend state with variables"
  },
  "conditional": {
    "prefix": ["if", "conditional"],
    "body": [
      "- conditional:",
      "    expression: \"$1\"",
      "    ifTrue:",
      "      - $2",
      "    ifFalse:",
      "      - $3"
    ],
    "description": "Conditional logic"
  },
  "repeat": {
    "prefix": ["rep", "repeat"],
    "body": [
      "- repeat:",
      "    times: $1",
