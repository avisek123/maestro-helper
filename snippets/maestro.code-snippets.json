    "body": [
      "- pressKey: Enter"
    ],
    "description": "Press Enter key"
  },
  "hideKeyboard": {
    "prefix": ["hk", "hidekeyboard"],
    "body": [
      "- hideKeyboard:"
    ],
    "description": "Hide keyboard"
  },
  "back": {
    "prefix": ["ba", "back"],
    "body": [
      "- back:"
    ],
    "description": "Press back button"
